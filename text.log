<div class="box-title"><h1 id="WebCam">&#x6E2C;&#x7AD9;&#x5468;&#x570D;&#x5F71;&#x50CF;</h1></div>

<div id="imgstore" style="xdisplay:none"> 
<img src="//taqm.epa.gov.tw/taqm/resource/pixel.gif" alt="" width="160px" height="120px" id="imgBox" style="cursor:pointer;">
<select id="WebCamBox_Imgs" style="font-size:12pt;width:160px;"></select>
<div id="showhere"></div>
<script type="text/javascript">
    var urlBase = "/taqm/tw/WebCam/";
    var urlImage = "//taqm.epa.gov.tw/taqm/webcam.ashx?type=s&site=";
    var TIME_OUT = 4000;
    var TIMER_ID = null;
    function getUrl() {
        var url = "";
        var params = $("#WebCamBox_Imgs").val().split("|");
        if (params.length==2) 
            url = params[0]; // + "&ts=" + (new Date().getTime());
            
        return urlImage + url;
    }
    
    $(document).ready(function() {
    	document.getElementById("imgBox").onerror = function() { 
            if ( TIMER_ID != null ) window.clearTimeout(TIMER_ID);
            $("#imgstore").hide();
    	}; 
    	
        // 載入資料
        $.get("../aqs.ashx?act=WebCamBox&lang=tw", function (data) {
            if ( data.Result == "ok" ) {
                $(data.Data).each(function( index ) {
                  addWebcam(this);
                });    
                
                fnNext();
            } // if
            else {
                if (window.console) console.log(data.Message)
            } // else
        });   
    
    });
    
    function addWebcam(data) {
        $("#WebCamBox_Imgs").append($("<option></option>").attr("value", data.SiteId+"|"+data.SiteKey).text(data.SiteText));
    }
    
    function fnNext() {
        // NEXT
        var v1 = $("#WebCamBox_Imgs").val();
        $('#WebCamBox_Imgs option:selected').next().attr('selected', 'selected');
        var v2 = $("#WebCamBox_Imgs").val();
        if ( v1 == v2 ) $('#WebCamBox_Imgs option:first').next().attr('selected', 'selected');
        
        show();
        
    }
    
function show() {
    if ( TIMER_ID != null ) window.clearTimeout(TIMER_ID);
    
    $("#imgBox").fadeTo(1000,0.30, function() {
      $("#imgBox").attr("src",getUrl());
      TIMER_ID = window.setTimeout(fnNext, TIME_OUT);
    }).fadeTo(500,1);
}    


$("#imgBox").click(function() {
    goPage();
});
    
$("#WebCamBox_Imgs").change(function(){
    var $me = $(this);
    var selectIndex = $me.find(":selected").index();
    
    if ( selectIndex == 0 )
        goPage();
    else
        show();
});

function goPage() {
    var params = $("#WebCamBox_Imgs").val().split("|");
    var url = urlBase;
    if (params.length==2)
        url += (params[1] + ".aspx");
    else
        url += "default.aspx";

    document.location.href = url;
}

</script>
